.index uni-nav-bar .uni-nav-bar.index-lf {
    width: 56rpx;
    height: 56rpx;
    background-image: url(data:image/png;base64,);
    background-size: 56rpx;
}
.index uni-nav-bar .uni-nav-bar.index-rg {
    width: 56rpx;
    height: 56rpx;
    background-image: url(data:image/png;base64,);
    background-size: 56rpx;
}
.index .top {
    width: 100%;
    height: 200rpx;
}
.index .top swiper-item .swiper-item.index-bg1 {
      width: 100%;
      height: 200rpx;
      background-color: #007AFF;
}
.index .top swiper-item .swiper-item.index-bg2 {
      width: 100%;
      height: 200rpx;
      background-color: #4CD964;
}
.index .top swiper-item .swiper-item.index-bg3 {
      width: 100%;
      height: 200rpx;
      background-color: #F0AD4E;
}
.index .nav {
    position: relative;
}
.index .nav .middle {
      height: 260rpx;
}
.index .nav .middle .swiper-item.index-bg4 {
        height: 220rpx;
        background-color: #fedcba;
        margin: 10rpx 20rpx;
        padding: 20rpx 0 0;
        border-radius: 10rpx;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-flex-wrap: wrap;
                flex-wrap: wrap;
}
.index .nav .middle .swiper-item.index-bg4 .swiper-item.icon {
          width: 25%;
          height: 100rpx;
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;
          /* flex-direction: column; */
          /* 上下对齐 */
          position: relative;
}
.index .nav .middle .swiper-item.index-bg4 .swiper-item.icon image {
            width: 48rpx;
            height: 48rpx;
            position: absolute;
            top: 28%;
            left: 50%;
            transform: translate(-50%, -50%);
            -webkit-transform: translate(-50%, -50%);
}
.index .nav .middle .swiper-item.index-bg4 .swiper-item.icon text {
            font-size: 22rpx;
            position: absolute;
            top: 68%;
            left: 50%;
            transform: translate(-50%, -50%);
            -webkit-transform: translate(-50%, -50%);
}
.index .nav .middle .swiper-item.index-bg5 {
        height: 220rpx;
        background-color: #abcdef;
}
.index .nav .dots {
      position: absolute;
      bottom: 20rpx;
      left: 50%;
      /* 这里一定要注意兼容不然很可能踩坑 */
      transform: translate(-50%, 0);
      -webkit-transform: translate(-50%, 0);
      z-index: 99;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
              flex-direction: row;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
              justify-content: center;
}
.index .nav .dots .dot {
        width: 24rpx;
        height: 8rpx;
        -webkit-transition: all .6s;
        transition: all .6s;
        background: rgba(0, 0, 0, 0.3);
        margin-right: 10rpx;
}
.index .nav .dots .active {
        width: 24rpx;
        height: 8rpx;
        background: rgba(255, 255, 255, 0.8);
}

